{
  "bootstrap": "bootstrap",
  "loopboxSenders": ["user", "bot"],
  "bundles": {
    "commsvat": {
      "sourceSpec": "vat-comms.js"
    },
    "vattpvat": {
      "sourceSpec": "vat-vattp.js"
    }
  },
  "vats": {
    "bootstrap": {
      "sourceSpec": "bootstrap.js"
    },
    "bot": {
      "sourceSpec": "vat-bot.js"
    },
    "botcomms": {
      "bundleName": "commsvat",
      "creationOptions": {
        "enableSetup": true,
        "useTranscript": false
      }
    },
    "botvattp": {
      "bundleName": "vattpvat"
    },
    "user": {
      "sourceSpec": "vat-user.js"
    },
    "usercomms": {
      "bundleName": "commsvat",
      "creationOptions": {
        "enableSetup": true,
        "useTranscript": false
      }
    },
    "uservattp": {
      "bundleName": "vattpvat"
    }
  }
}
